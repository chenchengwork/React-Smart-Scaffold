{
  "name": "React-Smart-Scaffold",
  "version": "0.0.1",
  "author": "chencheng <cheng.chen@tianjishuju.com>",
  "description": "React smart scaffold",
  "main": "src/index",
  "scripts": {
    "dev": "BUILD_DEV=1 node server.js",
    "check-code": "eslint ./src",
    "mock": "./node_modules/.bin/json-server --watch ./mock/db.json --routes ./mock/routes.json",
    "test": "BABEL_ENV=test jest --colors",
    "coverage": "BABEL_ENV=test jest --colors --coverage",
    "intl": "rimraf ./i18n-messages && BABEL_ENV=i18n babel --quiet ./src > /dev/null && babel-node --presets es2015 ./scripts/translateIntl.js && rimraf ./i18n-messages",
    "publish-alone": "BUILD_DEV=0 node ./scripts/publish.js ~/Desktop/webapp /static/",
    "publish": "BUILD_DEV=0 node ./scripts/publish.js ~/Desktop/webapp /"
  },
  "dependencies": {
    "antd": "^3.0.2",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "flex.css": "^1.1.7",
    "immutability-helper": "^2.5.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "onfire.js": "^1.0.6",
    "promise-polyfill": "^6.0.2",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "url-search-params-polyfill": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "cli-color": "^1.2.0",
    "copy-webpack-plugin": "^4.2.1",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.7",
    "css-to-string-loader": "^0.1.3",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-alloy": "^1.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "jest": "^21.2.1",
    "json-server": "^0.12.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "string-loader": "0.0.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "urlencode": "^1.1.0",
    "urllib-sync": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js(x)?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenchengwork/React-Smart-Scaffold.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenchengwork/React-Smart-Scaffold/issues"
  },
  "homepage": "https://github.com/chenchengwork/React-Smart-Scaffold#readme"
}
