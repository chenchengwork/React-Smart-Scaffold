{
  "name": "Solution-Platform-Web",
  "version": "1.0.0",
  "author": "chencheng <cheng.chen@tianjishuju.com>",
  "description": "解决方案前端",
  "main": "src/index",
  "scripts": {
    "dev": "BUILD_DEV=1 node server.js",
    "check-code": "eslint ./src",
    "test": "BABEL_ENV=test jest --colors",
    "coverage": "BABEL_ENV=test jest --colors --coverage",
    "intl": "rimraf ./i18n-messages && BABEL_ENV=i18n babel --quiet ./src > /dev/null && babel-node --presets es2015 ./scripts/translateIntl.js && rimraf ./i18n-messages",
    "publish-alone": "BUILD_DEV=0 node ./scripts/publish.js ~/Desktop/webapp /static/",
    "publish": "BUILD_DEV=0 node ./scripts/publish.js /data/webapp /"
  },
  "dependencies": {
    "antd": "^3.6.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "flex.css": "^1.1.7",
    "immutability-helper": "^2.7.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "onfire.js": "^1.0.6",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "url-search-params-polyfill": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.6",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "less": "2.7.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js(x)?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/TJDS-Project-To-Product/Solution-Platform-Web.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://gitee.com/TJDS-Project-To-Product/Solution-Platform-Web/issues"
  },
  "homepage": "https://gitee.com/TJDS-Project-To-Product/Solution-Platform-Web#readme"
}
